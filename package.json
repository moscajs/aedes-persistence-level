{
  "name": "aedes-persistence-level",
  "version": "8.0.1",
  "description": "LevelDB persistence for Aedes",
  "main": "persistence.js",
  "engines": {
    "node": ">=14.0.0"
  },
  "scripts": {
    "test": "standard && tape test.js | faucet",
    "release": "read -p 'GITHUB_TOKEN: ' GITHUB_TOKEN && export GITHUB_TOKEN=$GITHUB_TOKEN && release-it --disable-metrics"
  },
  "release-it": {
    "github": {
      "release": true
    },
    "git": {
      "tagName": "v${version}"
    },
    "hooks": {
      "before:init": [
        "npm run test"
      ]
    },
    "npm": {
      "publish": true
    }
  },
  "precommit": "test",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mcollina/aedes-persistence-level.git"
  },
  "keywords": [
    "aedes",
    "mqtt",
    "broker",
    "level",
    "leveldb"
  ],
  "author": "Matteo Collina <hello@matteocollina.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mcollina/aedes-persistence-level/issues"
  },
  "homepage": "https://github.com/mcollina/aedes-persistence-level#readme",
  "devDependencies": {
    "aedes": "^0.46.3",
    "aedes-persistence": "^9.0.3",
    "concat-stream": "^2.0.0",
    "faucet": "0.0.1",
    "level": "^8.0.0",
    "mqemitter": "^4.5.0",
    "pre-commit": "^1.2.2",
    "release-it": "^14.14.2",
    "standard": "^17.0.0",
    "tape": "^4.13.3",
    "through2": "^4.0.2"
  },
  "dependencies": {
    "aedes-packet": "^2.3.1",
    "msgpack-lite": "^0.1.26",
    "qlobber": "^6.0.0"
  }
}
